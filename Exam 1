// ตัวอย่าง การใช้งาน ข้อความ sticker
//----------------------------------------
//         1. ข้อความ 
//----------------------------------------
if(message=="hello")
{
  result ={
    "type": "text",
    "text": "Hello, Cute Boy"
  }
}
//-----------------------------------------
if(message=="ทำอะไร")
  result ={
    "type": "text",
    "text": "ไม่ได้ "+message+" Cute Boy"
  }
//-----------------------------------------
if(message=="สบายดีไหม")
  result ={
    "type": "text",
    "text": "แล้วนายล่ะ "+message+" ? Cute Girl"
  }
//-----------------------------------------
if(message=="ชื่ออะไร")
  result ={
    "type": "text",
    "text": "ฉันชื่อ cBot"
  }
//------------------------------------------
//     2. Text และ emoji
//------------------------------------------
if(message=="hi")
{
  result={
    "type": "text",
    "text": "สวัสดี emoji $",
    "emojis": [
      {
        "index": 13,
        "productId": "5ac1bfd5040ab15980c9b435",
        "emojiId": "002"
      }
    ]
  }
}
//-----------------------------------------
//      3. sticker
//-----------------------------------------
if(message=="ง่วงจัง")
{
  result={
        type: "sticker",
        packageId: 1,
        stickerId: 1
  }
}
//-----------------------------------------
if(message=="ok")
{
  result={
        type: "sticker",
        packageId: 789,
        stickerId: 10858
  }
      
}
//-----------------------------------------

